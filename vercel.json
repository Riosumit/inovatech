{
    "version": 2,
    "builds": [
        {
          "src": "interface/build/**",
          "use": "@vercel/static"
        },
        {
          "src": "staticfiles/**",
          "use": "@vercel/static"
        },
        {
          "src": "media/**",
          "use": "@vercel/static"
        },
        {
          "src": "manage.py",
          "use": "@vercel/python"
        }
      ],
      "routes": [
        { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
        { "src": "/media/(.*)", "dest": "/media/$1" },
        { "src": "/(.*)", "dest": "/manage.py" }
      ]
  }